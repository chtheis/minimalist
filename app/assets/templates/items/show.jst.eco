<button class="toggle icon-check" title="Check!" alt="Check!" tabindex="0"></button>
<span class="move icon-move" title="Move Item"></span>
<div class="view">
    <label contenteditable="true" class="label" tabindex="0"><%= @description %></label>
    <textarea class="edit" maxlength="255"><%= @description %></textarea>
</div>
<a class="destroy icon-trash" title="delete"></a>
